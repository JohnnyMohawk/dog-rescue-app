  <% if (!user) { %>
    <link rel="stylesheet" href="/stylesheets/siwg.css">
  <% } %>
</head>
<body>
  <nav>
    <% if (user) { %>
      <a href="/auth/logout">
        <button type="submit">LOGOUT</button>
      </a>
      <a 
      href="/dogs/new" <%- title === 'Add Dog' ? 'class="active"' : '' %>
    ><button type="submit">ADD DOG</button></a>
    <a 
      href="/dogs" <%- title === 'All Dogs' ? 'class="active"' : '' %>
    ><button type="submit">ALL DOGS</button></a>
    <% } else { %>
      <a href="/auth/google" class="siwg-link">
        <button type="submit" class="siwg">
          <img id="siwg-image" alt="Sign in with Google" />
        </button>
      </a>
    <% } %>
  </nav>
